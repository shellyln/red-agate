{
  "name": "red-agate-react-host",
  "private": false,
  "version": "0.2.11",
  "description": "Using react componetns from RedAgate.",
  "keywords": [
    "RedAgate",
    "react"
  ],
  "main": "./bin/index.js",
  "module": "./modules/index.js",
  "modules.root": "./modules",
  "types": "./bin/index.d.ts",
  "typings": "./bin/index.d.ts",
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "red-agate": "^0.2.11",
    "red-agate-math": "^0.2.11",
    "red-agate-svg-canvas": "^0.2.11",
    "red-agate-util": "^0.2.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "jasmine": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "shx": "^0.3.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  },
  "scripts": {
    "clean": "run-s clean:cjs clean:esm clean:spec",
    "clean:cjs": "rimraf ./bin",
    "clean:esm": "rimraf ./modules",
    "clean:spec": "rimraf ./bin.test",
    "build": "run-s build:cjs build:esm build:spec",
    "build:cjs": "tsc --module commonjs --target es2015 --outDir bin --declaration --declarationDir ./bin",
    "build:esm": "tsc --outDir modules --declaration --declarationDir ./modules",
    "build:spec": "webpack-cli --mode=development --config webpack.config.js",
    "build:spec2": "tsc -p tsconfig.spec.json --module commonjs --target es2015 --outDir bin.test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "jasmine",
    "prepublishOnly": "run-s clean build test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellyln/red-agate.git"
  },
  "author": "shellyln",
  "license": "ISC"
}
