{
  "name": "red-agate-math",
  "private": false,
  "version": "0.1.2",
  "description": "red-agate math library.",
  "keywords": [
    "finite field",
    "galois field",
    "finite extension field",
    "polynomial",
    "Reed-Solomon",
    "RS",
    "BCH",
    "CRC32"
  ],
  "main": "./bin/index.js",
  "module": "./modules/index.js",
  "modules.root": "./modules",
  "types": "./bin/index.d.ts",
  "typings": "./bin/index.d.ts",
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "red-agate-util": "^0.1.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "shx": "^0.3.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "scripts": {
    "clean": "run-s clean:cjs clean:esm clean:spec",
    "clean:cjs": "rimraf ./bin",
    "clean:esm": "rimraf ./modules",
    "clean:spec": "rimraf ./bin.test",
    "build": "run-s build:cjs build:esm build:spec",
    "build:cjs": "tsc --module commonjs --target es2015 --outDir bin --declaration --declarationDir ./bin",
    "build:esm": "tsc --outDir modules --declaration --declarationDir ./modules",
    "build:spec": "webpack-cli --mode=development --config webpack.config.js",
    "build:spec2": "tsc -p tsconfig.spec.json --module commonjs --target es2015 --outDir bin.test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "jasmine",
    "prepublishOnly": "run-s clean build test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellyln/red-agate-math.git"
  },
  "author": "shellyln",
  "license": "ISC"
}
