{
  "name": "red-agate-debug-env",
  "private": true,
  "version": "0.0.2",
  "description": "red-agate debug environment",
  "keywords": [],
  "main": "./bin/index.js",
  "module": "./modules/index.js",
  "modules.root": "./modules",
  "types": "./bin/index.d.ts",
  "typings": "./bin/index.d.ts",
  "engines": {
    "node": "9.x"
  },
  "dependencies": {},
  "peerDependencies": {
    "red-agate": "^0.0.8",
    "red-agate-barcode": "^0.0.12",
    "red-agate-svg-canvas": "^0.0.7",
    "red-agate-util": "^0.0.7"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "@types/node": "^8.0.57",
    "autoprefixer": "^7.2.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.1",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "lite-server": "^2.3.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "postcss-custom-properties": "^6.2.0",
    "postcss-loader": "^2.0.9",
    "postcss-nested": "^2.1.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "shx": "^0.2.2",
    "source-map-loader": "^0.2.3",
    "styled-components": "^2.2.4",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "start": "run-s start:node",
    "start2": "cat ./src/examples/barcode.data.json | node dist/app.js > ./debug/index.html",
    "start:web": "lite-server -c=bs-config.js",
    "start:node": "node dist/app.js",
    "clean": "run-s clean:esm clean:declarations clean:dist",
    "clean:esm": "rimraf ./modules",
    "clean:declarations": "rimraf ./declarations",
    "clean:dist": "rimraf ./dist",
    "build": "run-s build:dev",
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "webpack -p --config webpack.config.js",
    "build:esm": "tsc --outDir modules --declaration --declarationDir ./declarations",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "jasmine",
    "watch": "run-p watch:webpack watch:web",
    "watch:web": "run-s start:web",
    "watch:webpack": "webpack --config webpack.config.js --watch --env.watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellyln/red-agate-debug-env.git"
  },
