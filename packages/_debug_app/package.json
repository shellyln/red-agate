{
  "name": "red-agate-debug-env",
  "private": true,
  "version": "0.1.2",
  "description": "red-agate debug environment",
  "keywords": [],
  "main": "./dist/app.js",
  "module": "./modules/index.js",
  "modules.root": "./modules",
  "types": "./bin/index.d.ts",
  "typings": "./bin/index.d.ts",
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "express": "^4.16.3",
    "puppeteer": "^1.5.0"
  },
  "peerDependencies": {
    "red-agate": "^0.1.2",
    "red-agate-barcode": "^0.1.2",
    "red-agate-math": "^0.1.2",
    "red-agate-svg-canvas": "^0.1.2",
    "red-agate-util": "^0.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.3",
    "autoprefixer": "^8.6.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "lite-server": "^2.3.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "postcss-custom-properties": "^7.0.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "shx": "^0.3.0",
    "source-map-loader": "^0.2.3",
    "styled-components": "^3.3.2",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "start": "run-s start:node",
    "start:web": "lite-server -c=bs-config.js",
    "start:express": "node dist/app.js --express",
    "start:node": "node dist/app.js --debug --handler=/kanban",
    "start:call": "cat ./src/reports/barcode.data.json | node dist/app.js > ./debug/index.html",
    "clean": "run-s clean:esm clean:declarations clean:dist",
    "clean:esm": "rimraf ./modules",
    "clean:declarations": "rimraf ./declarations",
    "clean:dist": "rimraf ./dist",
    "build": "run-s build:dev",
    "build:dev": "webpack-cli --mode=development --config webpack.config.js",
    "build:prod": "webpack-cli --mode=production --config webpack.config.js",
    "build:esm": "tsc --outDir modules --declaration --declarationDir ./declarations",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "jasmine",
    "watch": "run-p watch:webpack watch:web",
    "watch:web": "run-s start:web",
    "watch:webpack": "webpack --config webpack.config.js --watch --env.watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellyln/red-agate.git"
  },
  "author": "shellyln",
  "license": "ISC"
}
